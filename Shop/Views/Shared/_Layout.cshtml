                        <li class="nav-item">
                            <a class="nav-link active" href="/Cars/List/fuel">Бензиновые</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="/ShopCart/">Корзина</a>
                        </li>
                    </ul>
                    <form class="d-flex" role="search">
                        <input class="form-control me-2" type="search" placeholder="Найти" aria-label="Поиск">
                        <button class="btn btn-outline-success" type="submit">Поиск</button>
                    </form>
                </div>
            </div>
        </nav>
    </header>
    <div class="container mt-50">

        @RenderBody()
        
    </div>
    <footer class="container mt-5">
        <p class="float-end"><a href="#"><ya-tr-span data-index="26-0" data-translated="false" data-source-lang="en" data-target-lang="ru" data-value="Back to top" data-translation="Наверх" data-ch="0" data-type="trSpan">Back to top</ya-tr-span></a></p>
